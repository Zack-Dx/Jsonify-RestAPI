{
    "name": "taste_teaser_api",
    "version": "1.0.0",
    "description": "A mock API for restaurant data, providing a tantalizing glimpse into the world of flavors and cuisines.",
    "type": "module",
    "main": "src/server.js",
    "scripts": {
        "dev": "nodemon src/server.js",
        "start": "node src/index.js",
        "format:check": "prettier ./src --check",
        "format:fix": "prettier ./src --write",
        "lint:check": "npx eslint ./src",
        "lint:fix": "npx eslint ./src --fix",
        "fix:all": "npm run format:fix && npm run lint:fix"
    },
    "keywords": [
        "node",
        "express",
        "RESTful",
        "rest-api",
        "restaurant",
        "food",
        "cuisine",
        "mock API",
        "flavors",
        "dining",
        "menu",
        "taste"
    ],
    "author": "Zack",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "nodemon": "^3.0.1",
        "prettier": "3.0.3"
    },
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "mongoose": "^7.5.2",
        "winston": "^3.10.0"
    }
}

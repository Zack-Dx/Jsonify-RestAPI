{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var":"warn",
        "no-console":"off",
        "func-names":"off"
    }
}
